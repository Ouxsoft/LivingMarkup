{
	"name" : "hxtree/livingMarkup",
	"description" : "LivingMarkup turns markup into code",
	"keywords" : [
		"livingMarkup",
		"xml",
		"php",
		"preprocessor",
		"template",
		"engine",
		"markup"
	],
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "hxtree",
			"homepage" : "https://github.com/hxtree",
			"role" : "Developer"
		}
	],
	"require" : {
		"php" : ">=7.1"
	},
	"minimum-stability" : "dev",
	"support" : {
		"issues" : "https://github.com/hxtree/livingMarkup/issues"
	},
	"autoload" : {
		"psr-4" : {
			"LivingMarkup\\" : "src/"
		}
	},
	"homepage" : "https://github.com/hxtree/LivingMarkup",
	"require-dev": {
		"phpunit/phpunit": "^8",
		"friendsofphp/php-cs-fixer": "^2.17@dev"
	},
	"scripts": {
		"test": [
			"phpunit --bootstrap vendor/autoload.php tests/."
		]
	}
}
